{
  "name": "vue-todo",
  "version": "1.0.0",
  "description": "todo list that use vue",
  "main": "index.js",
  "scripts": {
    "precommmit": "npm run eslint-fix",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.client.js",
    "build": "npm run clean && npm run build:client",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.client.js",
    "clean": "rimraf dist",
    "test-dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "eslint": "eslint --ext .js --ext .jsx --ext .vue client/",
    "eslint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/"
  },
  "author": "fridolph",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^8.2.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-node": "^6.0.1",
    "vue": "2.5.13"
  },
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-vue-jsx": "3.7.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "eslint-standard": "^1.0.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "postcss-loader": "2.0.9",
    "rimraf": "^2.6.2",
    "style-loader": "0.20.3",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "url-loader": "1.0.1",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "2.5.13",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
